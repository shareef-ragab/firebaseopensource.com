{
  "name": "firebaseopensource",
  "description": "https://firebaseopensource.com",
  "version": "1.0.0",
  "author": "Abraham Haskins <abeisgreat@abeisgreat.com>",
  "private": true,
  "scripts": {
    "fmt": "prettier --write *.css src/**/**/**.scss src/**/**.scss src/**/**.ts src/**/**/**.ts src/**.ts test/**/**.ts test/**/**.js *.js",
    "dev": "nuxt",
    "build": "npm run fmt && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "test": "npm run fmt && jest test --no-cache || true",
    "get-routes": "ts-node -O '{\"module\": \"commonjs\"}' src/scripts/routesList.ts",
    "gen": "nuxt generate",
    "deploy": "npm run build && firebase --project=default deploy",
    "deploy:hosting": "npm run build && firebase --project=default deploy --only hosting",
    "prerender": "ts-node prerender index.ts"
  },
  "dependencies": {
    "@nuxtjs/sitemap": "^0.1.1",
    "@types/fs-extra": "^4.0.5",
    "@types/html-minifier": "^1.1.30",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/shelljs": "^0.7.6",
    "chalk": "^2.3.0",
    "clipboard": "^1.7.1",
    "date-fns": "^1.29.0",
    "firebase": "^5.1.0",
    "fs-extra": "^4.0.2",
    "html-loader": "^0.5.1",
    "html-minifier": "^3.5.6",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^20.0.4",
    "jest-vue-preprocessor": "^1.1.0",
    "nuxt": "^1.4.2",
    "rendertron": "^1.1.0",
    "rxjs": "^5.5.2",
    "sass-loader": "^6.0.6",
    "spawn-rx": "^2.0.12",
    "ts-node": "^3.3.0",
    "vue": "2.5.17",
    "vue-server-renderer": "2.5.17",
    "vue-template-compiler": "2.5.17"
  },
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/cheerio": "^0.22.8",
    "@types/core-js": "^0.9.42",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.22",
    "chai": "^4.1.1",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^3.6.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "firebase-admin": "^5.13.1",
    "firebase-tools": "^6.1.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-sass": "^0.2.0",
    "prettier": "^1.5.3",
    "sass-loader": "^6.0.6",
    "serve": "^9.4.0",
    "style-loader": "^0.18.2",
    "superstatic": "^6.0.0",
    "ts-loader": "^2.3.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^3.1.11"
  },
  "jest": {
    "testRegex": "spec.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.html": "<rootDir>/test/__pre__/template",
      "^.+\\.(ts|tsx)$": "<rootDir>/test/__pre__/typescript"
    }
  }
}
